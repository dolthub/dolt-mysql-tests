# The function should return an empty string if len is 0.
SELECT LPAD('a', 0, 'x') AS result;
result

SELECT LPAD('a', 0, '') AS result;
result

SELECT LPAD('', 0, 'x') AS result;
result

SELECT LPAD('', 0, '') AS result;
result

